				{
					"name": "Create Binding",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var guid = (function() {",
									"  function s4() {",
									"    return Math.floor((1 + Math.random()) * 0x10000)",
									"               .toString(16)",
									"               .substring(1);",
									"  }",
									"  return function() {",
									"    return s4() + s4() + '-' + s4() + '-' + s4() + '-' +",
									"           s4() + '-' + s4() + s4() + s4();",
									"  };",
									"})();",
									"postman.setEnvironmentVariable(\"last_binding_guid\",guid());",
									"",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "basic",
							"basic": [
								{
									"key": "password",
									"value": "{{basic-pw}}",
									"type": "string"
								},
								{
									"key": "username",
									"value": "{{basic-user}}",
